    "test": "jest --forceExit --silent --collectCoverage",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "envalid": "7.3.1",
    "express": "4.18.2",
    "helmet": "6.0.1",
    "hpp": "0.2.3",
    "jsonwebtoken": "9.0.0",
    "morgan": "1.10.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.2",
    "openapi-types": "12.1.0",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.42",
